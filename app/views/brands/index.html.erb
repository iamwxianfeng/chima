<h1>品牌列表</h1>

<table>
  <tr>
    <th>名称</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @brands.each do |brand| %>
  <tr>
    <td><%= brand.name %></td>
    <td><%= link_to '显示', brand %></td>
    <td><%= link_to '编辑', edit_brand_path(brand) %></td>
    <td><%= link_to '删除', brand, confirm: '确定删除吗?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

