<h1>Listing forecast_msizes</h1>

<table>
  <tr>
    <th>Height</th>
    <th>Weight</th>
    <th>Chest</th>
    <th>Middle chest</th>
    <th>Shoulder</th>
    <th>Sleeve</th>
    <th>Neck</th>
    <th>Down chest</th>
    <th>Ass</th>
    <th>Crosspiece</th>
    <th>Foot</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @forecast_msizes.each do |forecast_msize| %>
  <tr>
    <td><%= forecast_msize.height %></td>
    <td><%= forecast_msize.weight %></td>
    <td><%= forecast_msize.chest %></td>
    <td><%= forecast_msize.middle_chest %></td>
    <td><%= forecast_msize.shoulder %></td>
    <td><%= forecast_msize.sleeve %></td>
    <td><%= forecast_msize.neck %></td>
    <td><%= forecast_msize.down_chest %></td>
    <td><%= forecast_msize.ass %></td>
    <td><%= forecast_msize.crosspiece %></td>
    <td><%= forecast_msize.foot %></td>
    <td><%= link_to 'Show', forecast_msize %></td>
    <td><%= link_to 'Edit', edit_forecast_msize_path(forecast_msize) %></td>
    <td><%= link_to 'Destroy', forecast_msize, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

